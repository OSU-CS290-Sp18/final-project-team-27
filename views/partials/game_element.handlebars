<article class="game_element">


  <img class="game_image" src="{{PhotoURL}}"></img>
  <div class="game_info">
    <h2 class="game_title">{{title}}</h2>
    <p class="game_price">{{price}}</p>
    <p class="game_category">{{categories}}</p>
    <p class="game_description_text">{{description}}</p>
  </div>

</article>
